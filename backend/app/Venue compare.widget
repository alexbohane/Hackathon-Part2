import { z } from "zod"

const Venue = z.strictObject({
  id: z.string(),
  name: z.string(),
  image: z.string(),
  alt: z.string(),
  location: z.string(),
  cost: z.enum(["$", "$$", "$$$"]),
})

const WidgetState = z.strictObject({
  venues: z.array(Venue),
})

export default WidgetState

